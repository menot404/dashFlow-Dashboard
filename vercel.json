{
    "git": {
        "deploymentEnabled": {
            "master": true,
            "main": true
        }
    },
    "github": {
        "enabled": true,
        "autoAlias": true,
        "silent": false,
        "autoJobCancelation": true
    },
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}